{
  "name": "excel-ai-agent",
  "version": "2.0.0",
  "description": "Advanced GPT-powered Excel Add-in for intelligent data analysis and automation",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run build:client && npm run build:manifest",
    "build:client": "echo 'Building client assets...'",
    "build:manifest": "node scripts/build-manifest.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write 'src/**/*.js' 'public/**/*.{html,css,js}'",
    "validate": "npm run lint && npm run test"
  },
  "keywords": ["excel", "office-js", "ai", "openai", "add-in", "data-analysis", "automation"],
  "author": "Parth Patel <parth@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParthPatelCa/Excel-AI-Agent.git"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
